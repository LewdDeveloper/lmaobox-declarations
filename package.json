{
    "name": "@lewddeveloper/lmaobox-declarations",
    "version": "2.0.0",
    "types": "lib/index.d.ts",
    "scripts": {
        "build": "tstl",
        "dev": "tstl --watch",
        "lint": "eslint --ext .ts lib",
        "format": "prettier --write \"lib/**/*.ts\"",
        "lint:fix": "eslint --fix --ext .ts lib",
        "prettier:check": "prettier --check \"lib/**/*.ts\"",
        "prettier:write": "prettier --write \"lib/**/*.ts\"",
        "precommit:check": "npm-run-all --silent prettier:check lint",
        "precommit:auto": "npm-run-all --silent lint:fix prettier:write"
    },
    "keywords": [
        "lmaobox"
    ],
    "author": "Nessie (https://github.com/LewdDeveloper)",
    "license": "MIT",
    "lint-staged": {
        "**/*": "prettier --write --ignore-unknown"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.52.0",
        "@typescript-to-lua/language-extensions": "^1.0.0",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.9.0",
        "eslint-config-standard-with-typescript": "^37.0.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-jsdoc": "^46.4.6",
        "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-import-resolver-node": "^0.3.7",
        "eslint-module-utils": "^2.8.0",
        "eslint-plugin-es-x": "^7.2.0",
        "eslint-scope": "^7.2.2",
        "eslint-visitor-keys": "^3.4.2",
        "npm-run-all": "^4.1.5",
        "prettier": "3.0.0",
        "typescript": "^5.1.6",
        "typescript-to-lua": "^1.17.0"
    },
    "directories": {
        "lib": "lib"
    },
    "dependencies": {},
    "repository": {
        "type": "git",
        "url": "git+https://github.com/LewdDeveloper/lmaobox-declarations.git"
    },
    "bugs": {
        "url": "https://github.com/LewdDeveloper/lmaobox-declarations/issues"
    },
    "homepage": "https://github.com/LewdDeveloper/lmaobox-declarations#readme"
